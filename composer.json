{
    "name": "unlooped/grid-tpl-bs4-bundle",
    "description": "unlooped Grid Bundle Template: Bootstrap 4",
    "license": "MIT",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Steffen Persch",
            "email": "s.persch@unlooped.com"
        }
    ],
    "require": {
        "unlooped/grid-bundle": "*"
    },
    "autoload": {
        "psr-4": {
            "Unlooped\\GridTplBs4Bundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Unlooped\\GridBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "bamarni/composer-bin-plugin": true
        },
        "sort-packages": true
    },
    "scripts": {
    }
}
